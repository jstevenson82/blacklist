<h1>Listing coupons</h1>

<table>
  <tr>
    <th>B</th>
    <th>Headline</th>
    <th>Body</th>
    <th>Expire</th>
    <th>Exclusion</th>
    <th>User</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @coupons.each do |coupon| %>
  <tr>
    <td><%= coupon.b_id %></td>
    <td><%= coupon.headline %></td>
    <td><%= coupon.body %></td>
    <td><%= coupon.expire %></td>
    <td><%= coupon.exclusion %></td>
    <td><%= coupon.user_id %></td>
    <td><%= link_to 'Show', coupon %></td>
    <td><%= link_to 'Edit', edit_coupon_path(coupon) %></td>
    <td><%= link_to 'Destroy', coupon, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Coupon', new_coupon_path %>
